{
	"name": "david63/userranks",
	"type": "phpbb-extension",
	"description": "Displays a list of user ranks and the members in that rank.",
	"version": "2.1.0-RC1",
	"time": "2019-06-22",
	"keywords": [
		"phpbb",
		"extension",
		"user",
		"rank"
	],
	"license": "GPL-2.0-only",
	"authors": [{
			"name": "David Wood",
			"role": "Developer"
		}],
	"require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "User ranks list",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0"
		},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "user_ranks_version_file.json",
			"ssl": false
    	}
	}
}
